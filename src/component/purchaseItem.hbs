<div class="row">
    <div class="col-4">
        <div class="mb-2">{{number}}</div>
        <h6>{{status}}</h6>
    </div>
    <div class="col-2">
        <div class="mb-2">{{amount.title}}</div>
        <h6>{{amount.value}}</h6>
    </div>
    <div class="col-4">
        {{#each images}}
        <img src="{{this}}">
        {{/each}}
    </div>
    <div class="col-2 mt-4">
        {{#with button}}
        <a href="link" class="btn {{#if isPrimary}} btn-primary {{/if}}">{{text}}</a>
        {{/with}}
    </div>
</div>
<hr class="mt-2 mb-4">