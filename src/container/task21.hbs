<main>
    <div class="card">
        <div class="card-body">
            <h1>Heading</h1>
            {{#with heading}}
            {{#with main}}
            <p>{{title}}</p>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-primary">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-primary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>

    <br>

    <div class="card">
        <div class="card-body">
            <h1>Heading second</h1>
            {{#with second}}
            <p>{{title}}</p>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-secondary">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-secondary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>

    <br>

    <div class="card">
        <div class="card-body">
            <h1>Heading subblock</h1>
            {{#with subblock}}
            <p>{{title}}</p>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-dark">{{solid}}</button>
            <br>
            <br>
            <button class="btn btn-outline-dark">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    {{/with}}

    <br>
    <br>
    <br>

    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <h1 class="text-center display-1">{{title}}</h1>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <p>{{title}}</p>
                    <p>{{text}}</p>
                    {{#with button}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/with}}
                    {{/with}}
                </div>
            </div>
            <br>
            <h6>{{header_text}}</h6>
            {{#with button}}
            <button class="btn btn-info">{{text}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>

    <br>
    <br>
    <br>


    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <p class="text-uppercase">{{title}}</p>
            <p class="text-lowercase">{{text}}</p>
            {{#with button}}
            <a href="{{url}}" target="_blank" class="btn btn-link">{{text}}</a>
            {{/with}}
            <br>
            <div class="card">
                <div class="card-body">
                    <ul>
                        {{#each list_name}}
                        <li>
                            <a href="{{url}}" target="_blank" class="btn btn-link">{{text}}</a>
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            {{/with}}
        </div>
    </div>
</main>