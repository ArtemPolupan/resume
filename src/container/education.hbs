<div>
    {{#with main}}
    <main>
        <section>
            {{> title big=true data=" My educationss"}}
            <ul class="ps-5">
                {{#each educations}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (Зараз в процесі) {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>

        <section>
            {{> title data=" My certificates"}}
            <ul class="ps-5">
                {{#each certificates}}
                <li>
                    {{name}} ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}

</div>