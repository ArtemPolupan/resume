<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-center">{{description}}</p>
    {{/with}}


    <div class="card">
        <div class="card-body">
            {{#with section}}
            {{#with about_us}}
            {{#with title_list}}
            <div class="text-end display-6">{{main}}</div>
            <br>
            <h2 class="text-end">{{sub}}</h2>
            {{/with}}
            <br>
            {{#with button}}
            <button class="btn btn-outline-info">{{text}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>

    <br>
    <br>

    <div class="card">
        <div class="card-body">
            {{#with advantage}}
            <div class="display-5">{{title}}</div>

            <br>

            <div class="card">
                <div class="card-body">
                    {{#with efficiency}}
                    <div class="display-4">{{title}}</div>
                    <br>
                    <h6>{{sub_title}}</h6>
                    <p>{{description}}</p>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>

            <br>

            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        {{#with innovation}}
                        <div class="display-4">{{title}}</div>
                        <br>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>
                    </div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>

            <br>

            <div class="card">
                <div class="card-body">
                    <div class="text-end">
                        {{#with collaboration}}
                        <div class="display-4">{{title}}</div>
                        <br>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>
                    </div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/with}}

    <br>
    <br>

    <div class="card">
        <div class="card-body">
            {{#with agreement}}
            <div class="display-5">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with info}}
                    <h5>{{sub_title}}</h5>
                    <br>
                    <hr>
                    {{#with detail}}
                    <p>{{text}}</p>
                    <button class="btn btn-success">{{button}}</button>
                    <hr>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
    {{/with}}

    {{/with}}
</main>